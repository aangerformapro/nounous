{
    "require": {
        "guzzlehttp/psr7": "^2.6",
        "joseluisq/gimage": "^4.0",
        "monolog/monolog": "^3.4",
        "nesbot/carbon": "^2.69",
        "ngsoft/slim-cookie-middleware": "^1.1",
        "ngsoft/tools": "^3",
        "oscarotero/env": "^2.1",
        "roots/wp-config": "^1.0",
        "selective/basepath": "^2.1",
        "slim/http": "^1.3",
        "slim/slim": "4.*",
        "symfony/var-dumper": "^6.3",
        "vlucas/phpdotenv": "^5.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "": "src/",
            "App\\": "app/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
